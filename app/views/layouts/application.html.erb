<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= stylesheet_link_tag  params[:controller], media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
  <link rel="shortcut icon" href="<%= asset_path 'img/favicon.png'%>">
  <link rel="apple-touch-icon" href="<%= asset_path 'img/favicon_60x60.png'%>">
  <link rel="apple-touch-icon" sizes="76x76" href="<%= asset_path 'img/favicon_76x76.png'%>">
  <link rel="apple-touch-icon" sizes="120x120" href="<%= asset_path 'img/favicon_120x120.png'%>">
  <link rel="apple-touch-icon" sizes="152x152" href="<%= asset_path 'img/favicon_152x152.png'%>">
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <link href="https://fonts.googleapis.com/css?family=Poppins:600,700" rel="stylesheet">
  
</head>

<body class="header-vertical">

    
    
    <% unless user_signed_in?%>
    <%= link_to "회원가입", new_user_registration_path %>
    <%= link_to "로그인", new_user_session_path%>
    <% else%>
    <%= link_to "로그아웃", destroy_user_session_path, method: :delete%>
    <% end%>
  
  
  <%= yield %>
  
  <div id="mobile-nav-toggle"><span></span><span></span><span></span></div>

  <%= javascript_include_tag params[:controller]%>
  <script src="//maps.google.com/maps/api/js?key=AIzaSyCS7UYWXc0p9-BvxaFlELVWP76DfPcDAyc"></script>  
  <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>  
  <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script>
  <script src='//underscorejs.org/underscore-min.js' type='text/javascript'></script>
  
  <%= javascript_include_tag 'footer' %>
  
</body>
</html>

